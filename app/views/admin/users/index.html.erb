<h1>Admin User's management</h1>
<table>
  <tr>
    <th>Full Name</th>
    <th>Email</th>
    <th>Created at</th>
    <th>Admin?</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.full_name %></td>
      <td><%= user.email %></td>
      <td><%= user.created_at %></td>
      <td>Debug:false</td>
    </tr>
  <% end %>
</table>